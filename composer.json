  "keywords": [
    "twitter",
    "scraper"
  ],
  "homepage": "https://github.com/sergix44/twscraper",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "campo/random-user-agent": "^1.3",
    "guzzlehttp/guzzle": "~7.4.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "SergiX44\\Twitter\\": [
        "src"
      ]
    }
  }
